/* pages/history_list/history_list.wxss */
.scroll_history_list{
  display: flex;
  justify-content: center;
}
.history_list{
  width: 92%;
  height: 310rpx;
  margin-left: 3%;
  margin-right: 5%;
  margin-top: 50rpx;
  border-style: solid;
  border-width: medium;
  border-color: #cccccc;
  border-radius: 4%;
  box-shadow: 1rpx 1rpx 1rpx 1rpx #666666;
  background: #f2f2f2;
}
.history_list_line1{
  display: flex;
  background: #f2f2f2;
  margin-top: 10rpx;
  border-radius: 4%;
  height: 58rpx;
}
.history_list_line1_item1{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
  border-radius: 10%;
  background: #d7d7d7;
  margin-left: 10rpx;
  font-size: 24rpx;
}
.history_list_line1_item1_2{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 140rpx;
