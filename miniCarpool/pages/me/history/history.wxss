  font-size: 24rpx;
  color:  black;
}
.history_list_line1_item2{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 500rpx;
  margin-left: 6%;
  background: #f2f2f2;
}
.clock{
  width: 32rpx;
